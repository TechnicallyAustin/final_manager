<h1> Login Here </h1>

<%= form_tag  do%>
<%=label_tag "Username"%>
<%=text_field_tag :username, nil, placeholder: "Username"%>

<%=label_tag "Password"%>
<%=password_field_tag :password, nil, placeholder: "Password"%>

<%=submit_tag "Log In"%>
<%end%>

<h3> Don't have an account?</h3>
<button><a href="/signup">Sign Up</a></button>